import java.util.Scanner;

public class Maindois{
    public static void main(String[] args) {
          Scanner sc = new Scanner(System.in);   
          System.out.print("Insira até qual valor vai sua sequência de FIBONACCI: ");
          int n = sc.nextInt();
          sc.close();
          int a = 0;
          int b = 1;
          int aux;
          for (int i =1; i <= n; i++){
            System.out.print(a + " - ");
            aux = a;
            a = a+b;
            b = aux;
                        
        }


    }
}
